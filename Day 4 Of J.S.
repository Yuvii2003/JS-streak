    <script>
        function sum(){
            var a1=prompt("Enter the 1st number to add: ");
            var b1=prompt("Enter the 2nd number to add: ");
            console.log("The sum of the given numbers is: "+(a1+b1));
        }
             function multi(){
            var a2=prompt("Enter the 1st number to multiply: ");
            var b2=prompt("Enter the 2nd number to multiply: ");
            console.log("The multipliction of numbers is: "+(a2*b2));
        }
       function subs(){
            var a3=prompt("Enter the 1st number: ");
            var b3=prompt("Enter the 2nd number: ");
            console.log("The difference between numbers is: "+(a3-b3));
        }
       function divide(){
            var a4=prompt("Enter the 1st number: ");
            var b4=prompt("Enter the 2nd number: ");
            console.log("The division between numbers is: "+(a4/b4));
        }
          function log(){
            var a5=prompt("Enter the number to get natural log: ");
            console.log(Math.log(a5));
        }
        function anti(){
            var a6=prompt("Enter a number to get antilog: ");
            console.log(Math.exp(a6));
        }
        function power(){
            var a7=prompt("Enter the number: ");
            a7=Number.parseInt(a7);
            a8=Number.parseInt(a8);
            var a8=prompt("Enter the power: ");
            console.log(a7**a8);
        }
        function sqrt(){
            var a9=prompt("Enter the number to get squareroot of that: ");
            console.log(Math.sqrt(a9));
        }
        sum();
        subs();
        multi();
        divide();
        log();
        anti();
        sqrt();
        power();
    </script>
